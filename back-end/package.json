{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "Image resize and processing API using Node.js, Express, and TypeScript",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "test": "node -r ts-node/register node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "keywords": [
    "image",
    "resize",
    "api",
    "typescript",
    "express",
    "sharp"
  ],
  "author": "yahye abdirashid",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "node-fetch": "^2.7.0",
    "sharp": "^0.32.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.6.5",
    "@types/node": "^20.19.24",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "jasmine": "^4.5.0",
    "jasmine-ts": "^0.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
